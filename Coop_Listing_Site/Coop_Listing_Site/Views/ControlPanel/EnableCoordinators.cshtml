
@{
    ViewBag.Title = "Enable Coordinators";
    Layout = "~/Views/Shared/_CP_Layout.cshtml";
}

<h2>Enable Coordinator Accounts</h2>



@if (ViewBag.Message != null)
{
    <p>@ViewBag.Message</p>
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal text-center">
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

        <div class="form-group">
            @Html.Label("Disabled Coordinators", new { @class = "control-label" })
            <br />
            <div class="">
                @Html.DropDownList("Coordinators", (MultiSelectList)ViewBag.Coordinators, new { multiple = "multiple" })
            </div>
        </div>

        <div class="form-group">
            <div>
                <input type="submit" value="Enable" class="btn btn-primary" />
            </div>
        </div>

    </div>

}